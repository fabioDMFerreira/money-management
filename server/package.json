{
    "name": "full-stack-backend",
    "version": "0.1.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "author": "Bowden Kelly",
    "license": "MIT",
    "scripts": {
        "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' -r tsconfig-paths/register src/main.ts",
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint",
        "serve": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --collectCoverage=true --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc -p tsconfig.production.json",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^6.1.1",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-flash": "0.0.1",
        "@types/jest": "^24.0.19",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.141",
        "@types/lusca": "^1.6.1",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.7.8",
        "@types/node-sass": "^4.11.0",
        "@types/nodemailer": "^6.2.1",
        "@types/passport": "^1.0.1",
        "@types/passport-facebook": "^2.1.9",
        "@types/passport-local": "^1.0.33",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "errorhandler": "^1.5.1",
        "eslint": "^6.4.0",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-jwt": "^5.3.1",
        "express-validator": "^6.2.0",
        "express-winston": "^4.0.1",
        "fbgraph": "^1.4.4",
        "inversify": "^5.0.1",
        "inversify-express-utils": "^6.3.2",
        "jwks-rsa": "^1.7.0",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "morgan": "^1.9.1",
        "pug": "^2.0.4",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "typescript": "^3.6.3",
        "tsconfig-paths": "^3.9.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "concurrently": "^4.1.2",
        "jest": "^24.9.0",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.2",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1"
    }
}
